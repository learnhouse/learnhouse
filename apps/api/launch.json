{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "poetry",
            "args": [
                "run","python" ,"app.py"
            ],
            "jinja": true
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/apps/api",
            "console": "integratedTerminal"
        }
    ]
}